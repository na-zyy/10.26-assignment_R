#06_TEST
multiples_of_pi<-new.env()
multiples_of_pi[["two_pi"]]<-c(2*pi)
multiples_of_pi$three_pi<-c(3*pi)
assign(
  "four_pi",
   4*pi,
  multiples_of_pi
)
ls(multiples_of_pi)
ls.str(multiples_of_pi)



h<-function(x){
  if(is.finite(x)==TRUE){
    if(x%%2==0)
      print("TRUE") else
        print("FALSE")
  }else{
    print("NA")
  }
}
h(9)
h(2)
h(-6)
h(0)
h(Inf)




hypotenuse<-function(x,y){
  sqrt(x^2+y^2)
}
g<-function(hypotenuse){
  two_element_list<-list(
    args<-args(hypotenuse),
    body<-body(hypotenuse)
  )
}


